{
    // word wrap settings    
        "editor.wordWrap": "on",
        "git.path": "/usr/local/bin/git",
    
    //-------- Remote VSCode configuration --------
    
    // Port number to use for connection.
    "remote.port": 52698,
    
    // Launch the server on start up.
    "remote.onstartup": true,
    
    // Prettier Javascript
    "editor.defaultFormatter": "esbenp.prettier-vscode",
      "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
